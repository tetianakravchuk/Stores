<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >



<suite name="TestSuite" parallel="none" >




    <test name="IETestCases">

        <parameter name="browser" value="ie"/>

        <classes>

            <class name ="RegistrationTests"/>
        </classes>

    </test>

    <test name="FirefoxTestCases">

        <parameter name="browser" value="firefox"/>

        <classes>

            <class name ="RegistrationTests"/>
        </classes>

    </test>

    <test name="ChromeTestCases">

        <parameter name="browser" value="chrome"/>

        <groups>
            <run>
                <exclude name="ie"></exclude>
                <include name="admin"></include>
                <include name="user"></include>
            </run>
        </groups>

        <classes>

            <class name ="RegistrationTests"/>
        </classes>

    </test>


</suite>


